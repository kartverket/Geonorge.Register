@model Kartverket.Register.Models.ViewModels.InspireReportViewModel

<script>
    @{var javaScriptSerializer = new System.Web.Script.Serialization.JavaScriptSerializer(); }

    var app = new Vue({
        el: '#inspireMonitoringApp',
        data: {
            current: @Html.Raw(HttpUtility.HtmlDecode(javaScriptSerializer.Serialize(Model.CurrentInspireMonitoring))),
            comparable: @Html.Raw(HttpUtility.HtmlDecode(javaScriptSerializer.Serialize(Model.ComparableCandidate))),
            compare: getParameterByName("compare") !== "" ? true : false,
        },
        computed: {
            currentDate: function () {
                if (this.current != null) {
                    return @Html.Raw(HttpUtility.HtmlDecode(javaScriptSerializer.Serialize(Model.CurrentInspireMonitoring.Date)));
                } else {
                    return "";
                }
            }
        }
    });

</script>